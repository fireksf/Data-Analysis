create database lwc_churchDB;
Use lwc_churchDB; --creating dababase 
--create tables 

Create table Members_list( 
ID int Auto_Increament Primary Key,
First_Name Varchar(50),
Last_Name Varchar(50),
DOB Date,
Gender Varchar(10),
Contact Varchar(12)

);
select  * from Program_list
Where Location ='Main Hall';

--group by
select  * from Believers_list
Where DateOfBirth > '1990-01-01'
and gender ='Male';

select Gender
from Believers_list
group by Gender;

--order by
select *
from member_list
order by firstname;

--limit 
select *
from member_list
where gender != 'male'
limit 6;


-- order and limit 
select *
from Believers_list
where gender != 'male'
order by dateofbirth desc
limit 6;
-- next possition of the current value by jumping the raw
select *
from Believers_list
limit 3, 1 -- this start raw 4
;
